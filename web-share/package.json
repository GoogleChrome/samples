{
  "name": "scrapbook-pwa",
  "version": "1.0.0",
  "devDependencies": {
    "dhost": "^0.1.7",
    "netlify-cli": "^2.15.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.0",
    "rollup": "^1.12.0",
    "rollup-plugin-copy": "^3.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-svelte": "^5.0.3",
    "svelte": "^3.0.0",
    "workbox-cli": "^5.0.0-beta.0"
  },
  "scripts": {
    "build": "run-s clean rollup workbox",
    "clean": "rimraf dist",
    "deploy": "run-s build netlify",
    "netlify": "netlify deploy --dir=dist --prod",
    "rollup": "rollup -c",
    "serve": "dhost --port=8080 dist",
    "workbox": "cd dist && workbox generateSW ../workbox-config.js && cd -"
  }
}
